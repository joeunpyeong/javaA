		System.out.print("  나  :");
		sc.nextLine();
	}
	//음성 수신
	void messageReceive() {
		System.out.print("상대방:");
		sc.nextLine();
	}
	//dmb방송 켜기
	public static void dmbOn() {
		
	}
	//dmb방송 끄기
	public static void dmbOff() {
		
	}
	//인터넷 켜기
	public static void internetOn() {
		
	}
	
	//인터넷 끄기
	public static void internetOff() {
		
	}
	
	//웹툰 보기
	public static void webtoonOn() {
		
	}
	
	//웹툰 끄기
	public static void webtoonOff() {
		
	}
}
