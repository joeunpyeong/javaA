package PhoneSimulator;

public class Main {	
	public static void main(String[] args) {
		SuperPhone phoneArr[] = new SuperPhone[3];

		phoneArr[0] = new Phone();
		phoneArr[1] = new DMB();
		phoneArr[2] = new SmartPhone();
		
		for (int i=0; i<phoneArr.length;i++) {
			phoneArr[i].inspect();
		}
	}
}